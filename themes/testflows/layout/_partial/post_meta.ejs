<meta name="image" property="og:image" content="<%- full_url_for(page.image.asset) %>">
<meta property="og:type" content="article">
<meta property="article:published_time" content="<%- new Date(page.date).toJSON().slice(0, 10) %>" />
<meta property="og:title" content="<%- page.title %>">
<meta property="og:image" content="<%- full_url_for(page.image.asset) %>">
<meta property="og:description" content="<%- page.description %>">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="<%- page.title %>">
<meta name="twitter:description" content="<%- page.description %>">
<meta name="twitter:image" content="<%- full_url_for(page.image.asset) %>">